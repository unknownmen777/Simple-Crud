@model IEnumerable< Login_Form.Models.Student>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table border="1">
    <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>GENDER</th>
        <th>AGE</th>
    </tr>
    @{ 
        foreach (var item in Model)
            {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Gender</td>
            <td>@item.Age</td>
        </tr>
            }
    }

</table>
